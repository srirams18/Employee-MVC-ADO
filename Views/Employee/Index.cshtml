@model List<Employee>

<h2>Employee List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Name</th>
            <th>DOB</th>
            <th>Gender<th>
            <th>Department</th>
            <th>Position</th>
            <th>salary</th>
            <th>Email</th>
            <th>phoneNumber</th>
            <th>Address</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.id</td>
                <td>@emp.Name</td>
                <td>@emp.DOB</td>
                <td>@emp.Gender</td>
                <td>@emp.Department</td>   
                <td>@emp.Position</td>
                <td>@emp.salary</td>
                <td>@emp.Email</td>
                <td>@emp.phoneNumber</td>
                <td>@emp.Address</td>
                 <td>
                    <!-- <a asp-action="Details" asp-route-id="@emp.id">Details</a> | -->
                    <a asp-action="Edit" asp-route-id="@emp.id">Edit</a> |
                    <a asp-action="Delete" asp-route-id="@emp.id">Delete</a>
                </td>

            </tr>
        }
    </tbody>
</table>

<a href="@Url.Action("Create")" class="btn btn-primary">Create New Employee</a>